{
  "semi": true,
  "trailingComma": "es5",
  "doubleQuote": true,
  "jsxDoubleQuote": true,
  "tabWidth": 2,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "proseWrap": "preserve",
  "plugins": ["prettier-plugin-tailwindcss"]
}
